def GIT_CREDENTIALS= "new-credentials"
pipeline{
    agent any
    stages{
            stage('checkout code'){
                steps{
                    script {
                    git credentialsID: GIT_CREDENTIALS,
                    url: 'https://github.com/ponnamanda-srinu/office-document.git', branch: 'main'
                    } 
                }
        }
    }
}
