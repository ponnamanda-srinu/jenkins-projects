def GIT_CREDENTIALS="ponnamanda-srinu"
pipeline{
    agent any
    stages{
        stage('stage1'){
            steps{
                'hello world'
            }
        }
            stage('checkout code'){
                steps{
                    sh 'git clone https://github.com/ponnamanda-srinu/jenkins-projects.git'
                    git credentialid:ponnamanda-srinu
                    branch:'master'       
                }
        }
    }
}
